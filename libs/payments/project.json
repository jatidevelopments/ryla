{
  "name": "payments",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/payments/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:lib"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/payments",
        "tsConfig": "libs/payments/tsconfig.lib.json",
        "main": "libs/payments/src/index.ts",
        "assets": ["libs/payments/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/payments"],
      "options": {
        "jestConfig": "libs/payments/jest.config.ts"
      }
    }
  }
}
