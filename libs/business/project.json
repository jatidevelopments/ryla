{
  "name": "business",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/business/src",
  "tags": ["type:lib", "scope:business"],
  "implicitDependencies": ["shared", "data"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/libs/business",
        "main": "libs/business/src/index.ts",
        "tsConfig": "libs/business/tsconfig.json",
        "assets": []
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run --config vitest.config.ts",
        "cwd": "libs/business"
      }
    }
  }
}
