{
  "1": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{baseImagePath}}"
    },
    "_meta": {
      "title": "Load Base Image"
    }
  },
  "2": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "flux1-schnell.safetensors"
    },
    "_meta": {
      "title": "Load Model"
    }
  },
  "3": {
    "class_type": "VAELoader",
    "inputs": {
      "vae_name": "ae.safetensors"
    },
    "_meta": {
      "title": "Load VAE"
    }
  },
  "4": {
    "class_type": "IPAdapterFaceID",
    "inputs": {
      "image": ["1", 0],
      "model": ["2", 0],
      "strength": 0.8
    },
    "_meta": {
      "title": "IPAdapter FaceID"
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}",
      "clip": ["2", 1]
    },
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{negativePrompt}}",
      "clip": ["2", 1]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "7": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "_meta": {
      "title": "Empty Latent"
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "seed": -1,
      "steps": 20,
      "cfg": 7.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["4", 0],
      "positive": ["5", 0],
      "negative": ["6", 0],
      "latent_image": ["7", 0]
    },
    "_meta": {
      "title": "KSampler"
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["3", 0]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "face_swap_output",
      "images": ["9", 0]
    },
    "_meta": {
      "title": "Save Image"
    }
  }
}

