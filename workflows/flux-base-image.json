{
  "1": {
    "class_type": "UNETLoader",
    "inputs": {
      "unet_name": "flux1-dev.safetensors",
      "weight_dtype": "default"
    },
    "_meta": {
      "title": "Load FLUX Diffusion Model"
    }
  },
  "2": {
    "class_type": "DualCLIPLoader",
    "inputs": {
      "clip_name1": "clip_l.safetensors",
      "clip_name2": "t5xxl_fp16.safetensors",
      "type": "flux",
      "device": "default"
    },
    "_meta": {
      "title": "Load FLUX Text Encoders"
    }
  },
  "3": {
    "class_type": "VAELoader",
    "inputs": {
      "vae_name": "ae.safetensors"
    },
    "_meta": {
      "title": "Load FLUX VAE"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}",
      "clip": ["2", 0]
    },
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{negativePrompt}}",
      "clip": ["2", 0]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "6": {
    "class_type": "FluxGuidance",
    "inputs": {
      "conditioning": ["4", 0],
      "guidance": 3.5
    },
    "_meta": {
      "title": "FLUX Guidance"
    }
  },
  "7": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "_meta": {
      "title": "Empty Latent"
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "seed": -1,
      "steps": 20,
      "cfg": 1.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["1", 0],
      "positive": ["6", 0],
      "negative": ["5", 0],
      "latent_image": ["7", 0]
    },
    "_meta": {
      "title": "KSampler"
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["3", 0]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "flux_base_image",
      "images": ["9", 0]
    },
    "_meta": {
      "title": "Save Image"
    }
  }
}

