{
  "_meta": {
    "title": "Flux Dev + IP-Adapter FaceID",
    "description": "Face consistency workflow using XLabs-AI Flux IP-Adapter v2 (designed for FLUX.1-dev)",
    "model": "flux1-dev",
    "adapter": "XLabs-AI Flux IP-Adapter v2",
    "author": "RYLA",
    "version": "1.0.0",
    "date": "2026-01-28"
  },
  "1": {
    "class_type": "UNETLoader",
    "inputs": {
      "unet_name": "flux1-dev.safetensors",
      "weight_dtype": "default"
    },
    "_meta": {
      "title": "Load FLUX Diffusion Model"
    }
  },
  "2": {
    "class_type": "DualCLIPLoader",
    "inputs": {
      "clip_name1": "t5xxl_fp16.safetensors",
      "clip_name2": "clip_l.safetensors",
      "type": "flux",
      "device": "default"
    },
    "_meta": {
      "title": "Load FLUX Text Encoders"
    }
  },
  "3": {
    "class_type": "VAELoader",
    "inputs": {
      "vae_name": "ae.safetensors"
    },
    "_meta": {
      "title": "Load FLUX VAE"
    }
  },
  "4": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{reference_image}}"
    },
    "_meta": {
      "title": "Load Reference Image"
    }
  },
  "6": {
    "class_type": "LoadFluxIPAdapter",
    "inputs": {
      "ipadatper": "ip_adapter.safetensors",
      "clip_vision": "model.safetensors",
      "provider": "CPU"
    },
    "_meta": {
      "title": "Load Flux IP-Adapter v2"
    }
  },
  "7": {
    "class_type": "ApplyFluxIPAdapter",
    "inputs": {
      "model": ["1", 0],
      "ip_adapter_flux": ["6", 0],
      "image": ["4", 0],
      "ip_scale": 0.8
    },
    "_meta": {
      "title": "Apply Flux IP-Adapter"
    }
  },
  "8": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}",
      "clip": ["2", 0]
    },
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "9": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{negative_prompt}}",
      "clip": ["2", 0]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "10": {
    "class_type": "EmptySD3LatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "_meta": {
      "title": "Empty Latent"
    }
  },
  "11": {
    "class_type": "KSampler",
    "inputs": {
      "seed": -1,
      "steps": 20,
      "cfg": 1.0,
      "sampler_name": "euler",
      "scheduler": "simple",
      "denoise": 1.0,
      "model": ["7", 0],
      "positive": ["8", 0],
      "negative": ["9", 0],
      "latent_image": ["10", 0]
    },
    "_meta": {
      "title": "KSampler"
    }
  },
  "12": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["11", 0],
      "vae": ["3", 0]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "13": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "flux_ipadapter_faceid",
      "images": ["12", 0]
    },
    "_meta": {
      "title": "Save Image"
    }
  }
}
