{
  "1": {
    "class_type": "UNETLoader",
    "inputs": {
      "unet_name": "flux1-dev.safetensors",
      "weight_dtype": "default"
    },
    "_meta": {
      "title": "Load FLUX Diffusion Model"
    }
  },
  "2": {
    "class_type": "DualCLIPLoader",
    "inputs": {
      "clip_name1": "clip_l.safetensors",
      "clip_name2": "t5xxl_fp16.safetensors",
      "type": "flux",
      "device": "default"
    },
    "_meta": {
      "title": "Load FLUX Text Encoders"
    }
  },
  "3": {
    "class_type": "VAELoader",
    "inputs": {
      "vae_name": "ae.safetensors"
    },
    "_meta": {
      "title": "Load FLUX VAE"
    }
  },
  "20": {
    "class_type": "PulidFluxModelLoader",
    "inputs": {
      "pulid_file": "pulid_flux_v0.9.1.safetensors"
    },
    "_meta": {
      "title": "Load PuLID Model"
    }
  },
  "21": {
    "class_type": "PulidFluxInsightFaceLoader",
    "inputs": {
      "provider": "CPU"
    },
    "_meta": {
      "title": "Load InsightFace"
    }
  },
  "22": {
    "class_type": "PulidFluxEvaClipLoader",
    "inputs": {},
    "_meta": {
      "title": "Load EVA CLIP"
    }
  },
  "23": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "{{referenceImage}}"
    },
    "_meta": {
      "title": "Load Reference Image"
    }
  },
  "24": {
    "class_type": "ApplyPulidFlux",
    "inputs": {
      "model": ["1", 0],
      "pulid_flux": ["20", 0],
      "eva_clip": ["22", 0],
      "face_analysis": ["21", 0],
      "image": ["23", 0],
      "weight": 0.8,
      "start_at": 0.0,
      "end_at": 1.0
    },
    "_meta": {
      "title": "Apply PuLID"
    }
  },
  "28": {
    "class_type": "FixPulidFluxPatch",
    "inputs": {
      "model": ["24", 0]
    },
    "_meta": {
      "title": "Fix PuLID Flux Patch"
    }
  },
  "30": {
    "class_type": "LoraLoader",
    "inputs": {
      "model": ["28", 0],
      "clip": ["2", 0],
      "lora_name": "{{loraName}}",
      "strength_model": 1.0,
      "strength_clip": 1.0
    },
    "_meta": {
      "title": "Load LoRA"
    }
  },
  "4": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{prompt}}",
      "clip": ["30", 1]
    },
    "_meta": {
      "title": "Positive Prompt"
    }
  },
  "5": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "{{negativePrompt}}",
      "clip": ["30", 1]
    },
    "_meta": {
      "title": "Negative Prompt"
    }
  },
  "6": {
    "class_type": "FluxGuidance",
    "inputs": {
      "conditioning": ["4", 0],
      "guidance": 3.5
    },
    "_meta": {
      "title": "FLUX Guidance"
    }
  },
  "7": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 1024,
      "batch_size": 1
    },
    "_meta": {
      "title": "Empty Latent"
    }
  },
  "8": {
    "class_type": "KSampler",
    "inputs": {
      "seed": -1,
      "steps": 20,
      "cfg": 1.0,
      "sampler_name": "euler",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["30", 0],
      "positive": ["6", 0],
      "negative": ["5", 0],
      "latent_image": ["7", 0]
    },
    "_meta": {
      "title": "KSampler (FLUX PuLID + LoRA)"
    }
  },
  "9": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["8", 0],
      "vae": ["3", 0]
    },
    "_meta": {
      "title": "VAE Decode"
    }
  },
  "10": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "final_generation_lora",
      "images": ["9", 0]
    },
    "_meta": {
      "title": "Save Image"
    }
  }
}

