{
  "name": "landing",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/landing",
  "projectType": "application",
  "tags": ["type:app", "scope:landing"],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/landing"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/landing"
        },
        "production": {}
      }
    },
    "pre-serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build:libs",
        "cwd": "{workspaceRoot}"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "landing:build",
        "dev": true,
        "port": 3003
      },
      "dependsOn": ["pre-serve", "^build"],
      "configurations": {
        "development": {
          "buildTarget": "landing:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "landing:build:production",
          "dev": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{workspaceRoot}/coverage/apps/landing"],
      "options": {
        "config": "apps/landing/vitest.config.ts"
      }
    }
  }
}
