# ======================
# RYLA Environment Variables
# ======================
# Copy this file to .env.local and fill in the values

# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# ComfyUI Pod (dedicated GPU for image generation)
# Get URL from RunPod Console → Pod → Connect → HTTP Service [Port 8188]
COMFYUI_POD_URL=https://your-pod-id-8188.proxy.runpod.net

# RunPod (for pod management only, not image generation)
RUNPOD_API_KEY=

# Finby Payments
FINBY_API_KEY=
FINBY_MERCHANT_ID=
FINBY_WEBHOOK_SECRET=

# AI (Replicate - optional fallback)
REPLICATE_API_TOKEN=

# Analytics
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com
